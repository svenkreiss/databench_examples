{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "databench-demo",
    "labels": {
      "app": "databench-demo"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "frontend",
        "image": "svenkreiss/databench_examples",
        "ports": [
          {
            "containerPort": 5000
          }
        ],
        "resources": {
          "limits": {
            "cpu": "20m"
          }
        }
      }
    ]
  }
}
